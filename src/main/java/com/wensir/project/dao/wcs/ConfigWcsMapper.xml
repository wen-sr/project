<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wensir.project.dao.wcs.ConfigWcsMapper" >
  <resultMap id="BaseResultMap" type="com.wensir.project.pojo.wcs.ConfigWcs" >
    <constructor >
      <arg column="PARA_NAME" jdbcType="VARCHAR" javaType="java.lang.String" />
      <arg column="PARA_VALUE" jdbcType="VARCHAR" javaType="java.lang.String" />
      <arg column="MARK" jdbcType="VARCHAR" javaType="java.lang.String" />
      <arg column="UPDATE_TIME" jdbcType="VARCHAR" javaType="java.lang.String" />
    </constructor>
  </resultMap>
  <insert id="insert" parameterType="com.wensir.project.pojo.wcs.ConfigWcs" >
    insert into CONFIG_WCS (PARA_NAME, PARA_VALUE, MARK, 
      UPDATE_TIME)
    values (#{paraName,jdbcType=VARCHAR}, #{paraValue,jdbcType=VARCHAR}, #{mark,jdbcType=VARCHAR}, 
      #{updateTime,jdbcType=VARCHAR})
  </insert>
  <insert id="insertSelective" parameterType="com.wensir.project.pojo.wcs.ConfigWcs" >
    insert into CONFIG_WCS
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="paraName != null" >
        PARA_NAME,
      </if>
      <if test="paraValue != null" >
        PARA_VALUE,
      </if>
      <if test="mark != null" >
        MARK,
      </if>
      <if test="updateTime != null" >
        UPDATE_TIME,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="paraName != null" >
        #{paraName,jdbcType=VARCHAR},
      </if>
      <if test="paraValue != null" >
        #{paraValue,jdbcType=VARCHAR},
      </if>
      <if test="mark != null" >
        #{mark,jdbcType=VARCHAR},
      </if>
      <if test="updateTime != null" >
        #{updateTime,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
  <update id="updateConfigWcsByParaName">
    update CONFIG_WCS
    set  PARA_VALUE = #{value}
    where PARA_NAME = #{key}
  </update>
  <select id="selectConfigWcsByParaName" resultType="com.wensir.project.pojo.wcs.ConfigWcs">
      select para_value paraValue
      from CONFIG_WCS
      where para_name = #{key}
    </select>
</mapper>